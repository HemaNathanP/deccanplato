<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Megam - Deccan</display-name>
	<servlet>
		<servlet-name>crm</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>classpath:applicationContext.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/list/*</url-pattern>
	</servlet-mapping>		
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/account/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/account/list/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/leads/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/list/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/accounts/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/accounts/list/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/leads/*</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/zoho/leads/list/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/sugar/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/sugar/list/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/sugar/accounts/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/sugar/relation/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm</servlet-name>
		<url-pattern>/provider/crm/sugar/leads/*</url-pattern>
	</servlet-mapping>
	
</web-app>